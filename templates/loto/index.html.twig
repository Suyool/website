<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
		{% block stylesheets %}
			{% for file in encore_entry_css_files('styles') %}
				<link rel="stylesheet" href="{{ asset(file|substr(1)) }}">
			{% endfor %}
		{% endblock %}
		<title>Loto</title>
	</head>
	<body class="loto">

		<div class="footer">
			<nav class="nav">
				<a href="#" class="nav__link active">
					<span class="nav__text">LLDJ</span>
				</a>
				<a href="#" class="nav__link radius">
					<span class="nav__text">PLAY</span>
				</a>
				<a href="#" class="nav__link">
					<span class="nav__text">RESULTS</span>
				</a>
			</nav>
		</div>
	</body>
	{% block javascripts %}
		{% for file in encore_entry_js_files('app') %}
			<script src="{{ asset(file|substr(1)) }}"></script>
		{% endfor %}
	{% endblock %}
</html>
